{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/mi-hogar/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/mi-hogar",
  "projects": {
    "articles": {
      "type": "application",
      "root": "apps/articles",
      "entryFile": "main",
      "sourceRoot": "apps/articles",
      "compilerOptions": {
        "tsConfigPath": "apps/articles/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "autocomplete": {
      "type": "application",
      "root": "apps/autocomplete",
      "entryFile": "main",
      "sourceRoot": "apps/autocomplete/src",
      "compilerOptions": {
        "tsConfigPath": "apps/autocomplete/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "developments": {
      "type": "application",
      "root": "apps/developments",
      "entryFile": "main",
      "sourceRoot": "apps/developments",
      "compilerOptions": {
        "tsConfigPath": "apps/developments/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "messaging": {
      "type": "library",
      "root": "libs/messaging",
      "entryFile": "index",
      "sourceRoot": "libs/messaging/src",
      "compilerOptions": {
        "tsConfigPath": "libs/messaging/tsconfig.lib.json"
      }
    },
    "mi-hogar": {
      "type": "application",
      "root": "apps/mi-hogar",
      "entryFile": "main",
      "sourceRoot": "apps/mi-hogar/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mi-hogar/tsconfig.app.json"
      }
    },
    "properties": {
      "type": "application",
      "root": "apps/properties",
      "entryFile": "main",
      "sourceRoot": "apps/properties",
      "compilerOptions": {
        "tsConfigPath": "apps/properties/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    }
  }
}